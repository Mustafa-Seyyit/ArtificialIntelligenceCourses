   "source": [
    "from collections import Counter\n",
    "\n",
    "numbers = [2, 3, 2, 5, 5, 3, 6]\n",
    "\n",
    "counts = Counter(numbers)\n",
    "repeatedItems = [num for num, cnt in counts.items() if cnt > 1]\n",
    "\n",
    "print(\"Tekrar eden sayılar:\", repeatedItems)\n"
   ]
