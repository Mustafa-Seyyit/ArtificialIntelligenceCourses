   ],
   "source": [
    "originalList = [i for i in range(10, 60, 10)]\n",
    "reversedList = []\n",
    "\n",
    "for i in range(len(originalList)-1, -1, -1):\n",
    "    reversedList.append(originalList[i])\n",
    "\n",
    "print(\"Orjinal dizi: \",  originalList)\n",
    "print(\"Tersine çevrilmiş Dizi: \", reversedList)"
   ]
  }
